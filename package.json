{
  "name": "doc-test",
  "version": "1.0.0",
  "main": "basicFunctions.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "comp": "tsc-watch",
    "dev": "tsc-watch --onSuccess \"env-cmd -f ./config/dev.env node dist/index.js\"",
    "doc": "typedoc --out ./docs ./src/**/*.ts",
    "test": "env-cmd -f ./config/test.env vitest run",
    "coverage": "env-cmd -f ./config/test.env vitest run --coverage --coverage.include 'src/**/*' --coverage.exclude src/db/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.0",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^3.0.5",
    "env-cmd": "^10.1.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "supertest": "^7.1.0",
    "tsc-watch": "^6.2.1",
    "typedoc": "^0.27.7",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "chalk": "^5.4.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mongoose": "^8.14.1",
    "validator": "^13.15.0",
    "yargs": "^17.7.2"
  }
}
